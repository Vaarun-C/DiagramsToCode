{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "MyConnectInstance": {
            "Type": "AWS::Connect::Instance",
            "Properties": {
                "InstanceAlias": "MyConnectInstance"
            }
        },
        "MyUserPool": {
            "Type": "AWS::Cognito::UserPool",
            "Properties": {
                "UserPoolName": "MyUserPool"
            }
        },
        "MyApiGateway": {
            "Type": "AWS::ApiGateway::RestApi",
            "Properties": {
                "Name": "MyAPI"
            }
        },
        "MyAmplifyApp": {
            "Type": "AWS::Amplify::App",
            "Properties": {
                "Name": "MyAmplifyApp",
                "Repository": "https://github.com/myrepo/myapp"
            }
        },
        "MyLexBot": {
            "Type": "AWS::Lex::Bot",
            "Properties": {
                "Name": "MyLexBot",
                "Locale": "en-US",
                "ChildDirected": false,
                "IdleSessionTTLInSeconds": 300
            }
        },
        "MyLambdaFunction": {
            "Type": "AWS::Lambda::Function",
            "Properties": {
                "Code": {
                    "ZipFile": "exports.handler = async () => 'OK';\n"
                },
                "Handler": "index.handler",
                "Runtime": "nodejs14.x",
                "Role": "arn:aws:iam::123456789012:role/MyLambdaRole"
            }
        },
        "MyDynamoDBTable": {
            "Type": "AWS::DynamoDB::Table",
            "Properties": {
                "AttributeDefinitions": [
                    {
                        "AttributeName": "Id",
                        "AttributeType": "S"
                    }
                ],
                "KeySchema": [
                    {
                        "AttributeName": "Id",
                        "KeyType": "HASH"
                    }
                ],
                "BillingMode": "PAY_PER_REQUEST"
            }
        },
        "MyFirehoseStream": {
            "Type": "AWS::KinesisFirehose::DeliveryStream",
            "Properties": {
                "DeliveryStreamName": "MyFirehoseStream",
                "S3DestinationConfiguration": {
                    "BucketARN": "arn:aws:s3:::my-destination-bucket",
                    "RoleARN": "arn:aws:iam::123456789012:role/MyFirehoseRole"
                }
            }
        },
        "MyS3Bucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
                "BucketName": "my-data-bucket"
            }
        },
        "MySageMakerModel": {
            "Type": "AWS::SageMaker::Model",
            "Properties": {
                "ExecutionRoleArn": "arn:aws:iam::123456789012:role/SageMakerRole",
                "PrimaryContainer": {
                    "Image": "123456789012.dkr.ecr.us-east-1.amazonaws.com/my-sagemaker-image:latest",
                    "Mode": "SingleModel"
                }
            }
        },
        "MyFHIRDatastore": {
            "Type": "AWS::HealthLake::FHIRDatastore",
            "Properties": {
                "DatastoreTypeVersion": "R4",
                "DatastoreName": "MyFHIRDatastore"
            }
        },
        "MyPinpointApp": {
            "Type": "AWS::Pinpoint::Application",
            "Properties": {
                "Name": "MyPinpointApp"
            }
        },
        "MyIAMRole": {
            "Type": "AWS::IAM::Role",
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Version": "2012-10-17",
                    "Statement": [
                        {
                            "Effect": "Allow",
                            "Principal": {
                                "Service": "lambda.amazonaws.com"
                            },
                            "Action": "sts:AssumeRole"
                        }
                    ]
                }
            }
        },
        "MyCloudWatchAlarm": {
            "Type": "AWS::CloudWatch::Alarm",
            "Properties": {
                "MetricName": "CPUUtilization",
                "Namespace": "AWS/EC2",
                "Period": 300,
                "EvaluationPeriods": 1,
                "Threshold": 80,
                "ComparisonOperator": "GreaterThanThreshold",
                "Statistic": "Average"
            }
        },
        "MyCloudTrail": {
            "Type": "AWS::CloudTrail::Trail",
            "Properties": {
                "TrailName": "MyCloudTrail",
                "S3BucketName": "my-cloudtrail-logs-bucket"
            }
        },
        "MyKMSKey": {
            "Type": "AWS::KMS::Key",
            "Properties": {
                "Description": "My KMS Key",
                "KeyPolicy": {
                    "Version": "2012-10-17",
                    "Statement": [
                        {
                            "Sid": "Enable IAM User Permissions",
                            "Effect": "Allow",
                            "Principal": {
                                "AWS": "arn:aws:iam::123456789012:root"
                            },
                            "Action": "kms:*",
                            "Resource": "*"
                        }
                    ]
                }
            }
        }
    }
}