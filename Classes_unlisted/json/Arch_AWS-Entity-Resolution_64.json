{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Basic CloudFormation template for AWS Entity Resolution.",
  "Resources": {
    "EntityResolutionJob": {
      "Type": "AWS::EntityResolution::Job",
      "Properties": {
        "JobName": "MyEntityResolutionJob",  // Replace with your job name
        "InputDataConfig": [
          {
            "InputDataType": "CSV",  // Specify the input data type
            "S3Uri": "s3://my-input-bucket/input-data.csv"  // Replace with your S3 input data URI
          }
        ],
        "OutputDataConfig": {
          "OutputDataType": "CSV",  // Specify the output data type
          "S3Uri": "s3://my-output-bucket/output-data.csv"  // Replace with your S3 output data URI
        },
        "RoleArn": "arn:aws:iam::123456789012:role/MyEntityResolutionRole",  // Replace with your IAM role ARN
        "EntityResolutionJobConfig": {
          "ResolutionJobType": "DEDUPE",  // Job type: DEDUPE or LINK
          "Attributes": [
            {
              "AttributeName": "name",  // Replace with relevant attributes
              "AttributeType": "STRING"
            }
          ]
        }
      }
    }
  },
  "Outputs": {
    "JobId": {
      "Description": "ID of the created Entity Resolution job.",
      "Value": {
        "Ref": "EntityResolutionJob"
      }
    }
  }
}
