{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Simple AWS Application Cost Profiler",
  "Resources": {
    "CostProfiler": {
      "Type": "AWS::ApplicationCostProfiler::CostProfiler",
      "Properties": {
        "Name": "MyCostProfiler",
        //remove while testing: "S3OutputPath": "s3://your-bucket-name/cost-profiler-output/",  // Replace with your S3 bucket path
        //remove while testing"Enable": true
      }
    }
  }
}
