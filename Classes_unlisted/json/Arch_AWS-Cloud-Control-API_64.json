{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Basic CloudFormation template for AWS Cloud Control API.",
  "Resources": {
    "MyResource": {
      "Type": "AWS::CloudControlApi::Resource",
      "Properties": {
        "TypeName": "AWS::S3::Bucket",  // Replace with the desired resource type
        "Properties": {
          "BucketName": "my-unique-bucket-name",  // Replace with your desired bucket name
          "VersioningConfiguration": {
            "Status": "Enabled"
          }
        }
      }
    }
  }
}
