{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Basic AWS CloudHSM Setup",
  "Resources": {
    "CloudHSMCluster": {
      "Type": "AWS::CloudHSM::Hsm",
      "Properties": {
        "ClusterId": "your-cluster-id",  // Replace with your existing cluster ID 
        "AvailabilityZone": "us-west-2a",  // Replace with your desired Availability Zone
        "HsmType": "hsm1.medium"           // Specify the HSM type ???????????????????
      }
    }
  },
  "Outputs": {
    "HsmArn": {
      "Description": "ARN of the CloudHSM HSM",
      "Value": {
        "Ref": "CloudHSMCluster"
      }
    }
  }
}
